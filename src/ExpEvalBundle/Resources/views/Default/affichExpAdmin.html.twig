<link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
<link href="{{ asset('assets/css/themify-icons.css') }}" rel="stylesheet">

{% endblock %}
<div class="wrapper">
    <div class="sidebar" data-background-color="white" data-active-color="danger">

        <!--
            Tip 1: you can change the color of the sidebar's background using: data-background-color="white | black"
            Tip 2: you can change the color of the active button using the data-active-color="primary | info | success | warning | danger"
        -->

        <div class="sidebar-wrapper">
            <div class="logo">
                <a href="http://www.creative-tim.com" class="simple-text">
BonPlan                </a>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="{{ path('bon_plan_accueilAdmin') }}">
                        <i class="ti-panel"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <li>
                    <a href="{{ path('annonce_indexback') }}">
                        <i class="ti-user"></i>
                        <p>Demande d'annonces</p>
                    </a>
                </li>
                <li>
                    <a href="{{  path('valider_Pub')  }}">
                        <i class="ti-view-list-alt"></i>
                        <p>Demandes Publicites</p>
                    </a>
                </li>
                <li>
                    <a href="{{ path('List_Admin') }}">
                        <i class="ti-text"></i>
                        <p>liste des événements</p>
                    </a>
                </li>
                <li>
                    <a href="{{ path('bon_plan_categorie') }}">
