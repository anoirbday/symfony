<div class="wrapper">
    <div class="sidebar" data-background-color="white" data-active-color="danger">

        <!--
            Tip 1: you can change the color of the sidebar's background using: data-background-color="white | black"
            Tip 2: you can change the color of the active button using the data-active-color="primary | info | success | warning | danger"
        -->

        <div class="sidebar-wrapper">
            <div class="logo">
                <a href="http://www.creative-tim.com" class="simple-text">
                    Creative Tim
                </a>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="#">
                        <i class="ti-panel"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="ti-user"></i>
                        <p>User Profile</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="ti-view-list-alt"></i>
                        <p>Ajouter un article</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="ti-text"></i>
                        <p>liste des événements</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="ti-pencil-alt2"></i>
                        <p>Liste des articles</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="ti-map"></i>
                        <p>Liste reclamation</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="ti-bell"></i>
                        <p>Notifications</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="backcornertop"></i>
                        <p>lister categ</p>
                    </a>
                </li>

                <li class="active-pro">
                    <a href="#">
                        <i class="ti-export"></i>
                        <p>ajouter categ</p>
                    </a>
                </li>
            </ul>
        </div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default">

            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar bar1"></span>
                        <span class="icon-bar bar2"></span>
                        <span class="icon-bar bar3"></span>
                    </button>
                    <a class="navbar-brand" href="#">Dashboard</a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li><div style="color: white;
padding: 15px 50px 5px 50px;
float: right;
font-size: 16px;" class="username">
                                {{app.user.username}} &nbsp;
                                <a href="{{ path('fos_user_security_logout') }}" class="btn btn-danger square-btn-adjust">Logout</a>
                            </div></li>
                        <li>


                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">statistique
                                <i class="ti-panel"></i>
                                <ul class="dropdown-menu">

                                    <li><a href="#">statistique en batton</a></li>
                                    <li><a href="#">statistique en pie</a></li>
                                    <li><a href="#">statistique en ligne</a></li>

                                </ul>




                        <li>
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Notifications
                                <i class="ti-bell"></i></a>


                            <ul class="dropdown-menu">


                                <li><a href="#">Notification evenement en cours</a></li>
                                <li><a href="#">Notification evenement du jour</a></li>
                                <li> <a href="#">Notification nouveau produit</a></li>


                            </ul>
                        </li>
                        <li>
                            <a href="#">
                                <i class="ti-settings"></i>
                                <p>Settings</p>
                            </a>
                        </li>
                    </ul>

                </div>
            </div>
            {% endblock %}
        </nav>

        {% block body %}
            <div class="content">

                <div class="row">

                            <div class="header">
                                <h4 class="title">{{ etablissement.nomEtablissement }}</h4>
                            </div>
                            <div class="content">
                                    <table class="table">
                                        <tbody aria-rowspan="300px">

                                        <tr>
                                            <th>Adresse établissement:</th>
                                            <td>{{ etablissement.adresseEtablissement }}</td>
                                        </tr>
                                        <tr>
                                            <th>Telephone établissement:</th>
                                            <td>{{ etablissement.telephoneEtablissement }}</td>
                                        </tr>
                                        <tr>
                                            <th>Horaire travail:</th>
                                            <td>{{ etablissement.horaireTravail }}</td>
                                        </tr>
                                        <tr>
                                            <th>Description établissement:</th>
                                            <td>{{ etablissement.descriptionEtablissement }}</td>
                                        </tr>
                                        <tr>
                                            <th>Photo établissement:</th>
                                            <td> <img style="height: 250px;width: 350px" src="{{ asset('uploads/images/' ~ etablissement.photoEtablissement ) }}" alt="" class="img-responsive">
                                            </td>
                                        </tr>

                                        <tr>
                                            <th>Photo patente:</th>
                                            <td> <img style="height: 350px;width: 350px" src="{{ asset('uploads/images/' ~ etablissement.photoPatente ) }}" alt="" class="img-responsive">
                                            </td>
                                        </tr>
                                        <tr>
                                            <th>Code postal:</th>
                                            <td>{{ etablissement.codePostal }}</td>
                                        </tr>
                                        <tr>
                                            <th>Budget:</th>
                                            <td>{{ etablissement.budget }}</td>
                                        </tr>
                                        <tr>
                                            <th>Site web:</th>
                                            <td>{{ etablissement.siteWeb }}</td>
                                        </tr>

                                        <tr>
                                            <th>Longitude:</th>
                                            <td>{{ etablissement.longitude }}</td>
                                        </tr>
                                        <tr>
                                            <th>Latitude:</th>
                                            <td>{{ etablissement.latitude }}</td>
                                        </tr>
                                        </tbody>
                                    </table>



                                <ul>
                                    <a data-basealign="right" class="btn btn-danger square-btn-adjust" href="{{ path('bon_plan_etablissement8',{'idEtablissement': etablissement.idEtablissement}) }}">Rejeter demande</a>
                                    <a class="btn btn-danger square-btn-adjust" href="{{ path('bon_plan_etablissement9',{'idEtablissement': etablissement.idEtablissement}) }}">Valider demande</a>

                                </ul>

                                <div class="footer">
                                    <div class="chart-legend">
                                        <i class="fa fa-circle text-warning"></i>  <a href="{{ path('bon_plan_etablissement6') }}">Retour à la liste</a>
                                    </div>
                                    <hr>
                                </div>
                            </div>


                </div>
            </div>
        {% endblock %}
    </div>


    <footer class="footer">
        {% block footer %}
        <div class="container-fluid">
            <nav class="pull-left">
                <ul>

                    <li>
